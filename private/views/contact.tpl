<div id="title">
    <h1>Contact</h1>
</div>

<div id="contactform">
    <form>
        <label for="fname">First Name</label>
        <input type="text" id="fname" name="firstname" placeholder="first name"><br><br>

        <label for="lname">Last Name</label>
        <input type="text" id="lname" name="lastname" placeholder="last name"><br><br>

        <label for="subject">Subject</label><br><br>
        <textarea id="subject" name="subject" placeholder="Write your message..."></textarea><br>

        <input type="submit" name="emailsubmit" value="Submit">
    </form>
</div>